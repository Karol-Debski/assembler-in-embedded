#include <stdint.h>
#include <string.h>

void memcopy_asm(void* dst, void* src, int num);

uint8_t tab1[] = {1, 2, 3, 4, 5, 6};
uint8_t tab2[] = {0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF};

uint8_t str[] = "Hello World!";
uint8_t str_buff[16]= {0};

int main(void)
{
	uint32_t num_bytes = 5;

    volatile __asm__("ldr r0, =tab1\n\t"
    				 "ldr r1, =tab2\n\t"
    				 "ldr r2, =tab1\n\t" ::: );
	memcopy_asm(tab1, tab2, 6);

	memcopy_asm(str_buff, str, sizeof(str));

    /* Loop forever */
	for(;;);
}

  .syntax unified
  .cpu cortex-m4
  .fpu softvfp
  .thumb

.global  main

.data
 tab1: .word 1, 2, 3, 4, 5, 6, 7, 8
 tab2: .fill 8, 4, 0xFFFFFFFF

.extern memcpy

.text

main:

 //dst
 movw r0, :lower16:tab1
 movt r0, :upper16:tab1
 //src
 movw r1, :lower16:tab2
 movt r1, :upper16:tab2
 //size in bytes
 mov r2, 8


 bl memcpy

loop:
 b loop


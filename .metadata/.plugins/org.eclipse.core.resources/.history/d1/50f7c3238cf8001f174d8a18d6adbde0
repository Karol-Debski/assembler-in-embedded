#include <stdint.h>
#include <string.h>


uint8_t tab1[] = {1, 2, 3, 4, 5, 6};
uint8_t tab2[] = {0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF};


int main(void)
{
	uint32_t num_bytes = 5;

	__asm__ volatile ("ldr r0, =tab1\n\t"
    				  "ldr r1, =tab2\n\t"
    				  "mov r2, %0\n\t"
					  "bl memcopy_asm\n\t"
					  :
    				  : "r" (num_bytes)
					  : "r0", "r1", "r2");


    /* Loop forever */
	for(;;);
}

  .syntax unified
  .cpu cortex-m4
  .fpu softvfp
  .thumb

.global  main

.include stm32f407xx.h

// led i timer stworz
// main i przerwanie w asm
// wywolaj wszystko w .S

.data
 gpioLEDHandler: .word GPIOD
 				 .byte GPIO_PIN_NUM_14
 				 .byte GPIO_MODE_OUTPUT
 				 .byte 0
 				 .byte 0
 				 .byte GPIO_OUTPUT_TYPE_PP
 				 .byte 0
 .align 4

.text



main:

 bx lr

/*--------------*/
SysTick_Handler:
 push {lr}
 bl flip_led_state
 pop {lr}

 bx lr

#include <stdint.h>
#include <string.h>

void memcopy_asm(void* dst, void* src, int num);

uint8_t tab1[] = {1, 2, 3, 4, 5, 6};
uint8_t tab2[] = {0xFF, 0xFF, 0xFF, 0xFF, 0xFF, 0xFF};

const uint8_t str[] = "Hello World!";
uint8_t str_buff[16]= {0};

int main(void)
{
    //void* ret = 0;
	memcopy_asm(tab1, tab2, 6);

	memcopy_asm(str_buff, str, strlen(str));

    /* Loop forever */
	for(;;);
}

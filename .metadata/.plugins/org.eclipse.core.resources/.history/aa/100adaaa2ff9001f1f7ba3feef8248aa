uint32_t clz_inline(uint32_t word)
{
	__asm__ volatile ("clz r0, r0":::);
	return;
}

uint32_t rev_inline(uint32_t word)
{
	__asm__ volatile ("rev r0, r0":::);
	return;
}

uint32_t rbit_inline(uint32_t word)
{
	__asm__ volatile ("rbit r0, r0":"+r" (word));
	return word;
}


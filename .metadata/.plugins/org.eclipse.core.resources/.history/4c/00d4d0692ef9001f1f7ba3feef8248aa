clz_asm:
 clz r0, r0
 bx lr

rev_asm:
 rev r0, r0
 bx lr

rbit_asm:
 rbit r0, r0
 bx lr


void clz_inline(uint32_t word)
{
	__asm__ volatile ("clz r0, r0":::"lr");
}

